{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "SvelteKit dev (inspect)",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "env": { "NODE_OPTIONS": "--inspect=127.0.0.1:9400" },
        "skipFiles": ["<node_internals>/**"]
      }
    ]
  }
  